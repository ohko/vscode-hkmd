{
	"name": "hkmd",
	"displayName": "hkmd",
	"description": "",
	"version": "0.0.14",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Other"
	],
	"publisher": "ohko",
	"repository": "https://github.com/ohko/vscode-hkmd",
	"activationEvents": [
		"onCommand:hkmd.search",
		"onCommand:hkmd.stock"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "hkmd.search",
				"title": "HK Markdown docs"
			},
			{
				"command": "hkmd.stock",
				"title": "HK Stock Watch Open/Close",
				"icon": "media/dep.png"
			},
			{
				"command": "stockList.click",
				"title": "Detail"
			}
		],
		"configuration": {
			"title": "hkmd",
			"properties": {
				"hkmd.cookie": {
					"type": "string",
					"default": "",
					"description": "cookie"
				},
				"hkmd.stockRefresh": {
					"type": "number",
					"default": 8,
					"description": "stock refresh time of second"
				},
				"hkmd.stock": {
					"type": "array",
					"default": [],
					"description": "sh000001,sz000001"
				}
			}
		},
		"views": {
			"explorer": [
				{
					"id": "stockList",
					"name": "Stock List"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "hkmd.stock",
					"when": "view == stockList",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.6.4",
		"@types/vscode": "^1.48.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"eslint": "^7.6.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"axios": "^0.19.0"
	}
}